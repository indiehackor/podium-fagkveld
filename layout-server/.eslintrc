{
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "strict": [0, "global"],
        "class-methods-use-this": [0],
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "no-underscore-dangle": "off"
    },
    "overrides": [
        {
            "files": "src/lib/client/**/*",
            "parserOptions": {
                "sourceType": "module"
            },
            "env": {
                "browser": true
            }
        }
    ]
}
